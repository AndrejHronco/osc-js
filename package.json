{
  "name": "osc-js",
  "version": "1.0.0",
  "description": "OSC protocol interface for js",
  "main": "dist/osc.js",
  "scripts": {
    "lint": "eslint build.js src/** test/**; exit 0",
    "test": "mocha test/** --compilers js:babel-register; exit 0",
    "test:watch": "mocha test/** --compilers js:babel-register --reporter min --watch test/** src/**",
    "build": "rollem -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/adzialocha/osc-js.git"
  },
  "keywords": [
    "OSC",
    "data",
    "open",
    "sound",
    "control"
  ],
  "author": "adzialocha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adzialocha/osc-js/issues"
  },
  "files": [
    "dist",
    "lib"
  ],
  "homepage": "https://github.com/adzialocha/osc-js#readme",
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "eslint": "^3.9.1",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.1.0",
    "mocha": "^3.1.2",
    "rollem": "^1.7.0",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-uglify": "^1.0.1"
  }
}
